{
  "name": "doink-todo-app",
  "version": "1.0.0",
  "type": "module",
  "description": "A beautiful and functional to-do list app with reminders",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "setup:dynamodb": "node aws-setup/create-dynamodb-table.js",
    "setup:cognito": "node aws-setup/create-cognito-pool.js",
    "setup:aws": "npm run setup:dynamodb && npm run setup:cognito",
    "sandbox": "npx ampx sandbox --once",
    "sandbox:local": "AMPLIFY_EXTERNAL_PROVIDERS=false npx ampx sandbox --once",
    "sandbox:delete": "npx ampx sandbox delete"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.913.0",
    "@aws-sdk/client-dynamodb": "^3.913.0",
    "@aws-sdk/lib-dynamodb": "^3.913.0",
    "aws-amplify": "^6.15.8",
    "date-fns": "^2.29.3",
    "framer-motion": "^10.12.16",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.5.0",
    "@aws-amplify/backend-cli": "^1.2.9",
    "@aws-amplify/cli": "^14.2.3",
    "@aws-amplify/plugin-types": "^1.1.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "aws-cdk": "^2.138.0",
    "aws-cdk-lib": "^2.138.0",
    "constructs": "^10.3.0",
    "eslint": "^8.36.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.2.0"
  },
  "keywords": [
    "todo",
    "react",
    "reminders",
    "productivity"
  ],
  "author": "Your Name",
  "license": "MIT"
}
